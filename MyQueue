package com.company;

public class MyQueue<T extends Comparable<T>> {
    MyArrayList<T> arr = new MyArrayList<>();
    private int length = 0;
    private int front = 0;
    private int rear = 0;

    public boolean isEmpty(){
        if(rear == front){
            return true;
        }
        return false;
    }

    public T enqueue(T item){
        arr.add(item);
        rear++;
        length++;
        return (T)arr;
    }

    public T dequeue(){
        arr.remove(front);
        rear--;
        length--;
        return (T)arr;
    }

    public T peek(){
        return arr.get(rear);
    }

    public int size(){
        return rear - front;
    }
}
